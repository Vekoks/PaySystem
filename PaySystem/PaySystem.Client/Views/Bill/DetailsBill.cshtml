@model List<PaySystem.Client.Models.StatusBillModels.StatusBillModel>

@{
    WebGrid grid = new WebGrid(Model);
}


<br />
<div class="row">

    <div class="well col-md-6 col-md-offset-3">
        <h2 class="text-center">Status Bill</h2>

        <br />
        @grid.GetHtml(
    tableStyle: "table table-striped table-hover form-group",
    headerStyle: "text-center",
    columns: grid.Columns(
        grid.Column("Action", "Action"),
        grid.Column("ActionDate", "Date"),
        grid.Column("ActionResoult", "Status")
      ))
    </div>
</div>